<%- include("./partials/header.ejs")%>
<div class="container">
  <h1>Home</h1>
  <p><%=startContent%></p>
  <%blogs.forEach(blog => {%>
  <h1><%=blog.postTitle%></h1>
  <p>
    <%=blog.postContent.substring(0, 100) + " ..." %>
    <a href="/posts/<%=blog.postTitle%>">Read More</a>
  </p>
  <%})%>
</div>
<%- include("./partials/footer.ejs")%>
